<head>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta charset="UTF-8">
    <link href="/static/css/styles.css" rel="stylesheet" type='text/css'</link>
    <link href="https://fonts.googleapis.com/css?family=Lato&display=swap" rel="stylesheet">
    <title>Matchie</title>
</head>

<body id="manvrouw">
    <img src="/static/img/logo-datingssite.png" class="logo" alt="Matchie logo">

  <header>
    <nav>
      <a href="/provincie/<%= id %>" class="backButton"><img src="/static/img/nav_pijltje.png" class="pijltjeTerug" alt="navigatie naar vorige pagina"></a>
      <h1 class="stappen">Stap 5 / 7</h1>
    </nav>
  </header>

  <form action="/sendFormGeslacht" method="post">
    <fieldset>
      <legend>Ik ben een...</legend>
      <div class="gender">
        <input id="man" type="radio" name="gender" value="Man" required />
        <label for="man">Man</label>

        <input id="vrouw" type="radio" name="gender" value="Vrouw" required />
        <label for="vrouw">Vrouw</label>

        <input type="hidden" name="id" value="<%= id %>" />

      </div>
      <p>Woohoo! Je bent bijna klaar!</p>

      <button name="button">Doorgaan</button>
    </fieldset>
  </form>

<script src="/static/javascript/matchie.js"type="text/javascript"></script>
</body>
